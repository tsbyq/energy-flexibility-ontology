<https://www.auto.tuwien.ac.at/downloads/thinkhome/ontology/EnergyResourceOntology.owl>
  a owl:Ontology ;
  owl:versionInfo "1.03 EnergyResource Ontology"^^xsd:string ;
  rdfs:comment "Home automation conceptualization is based on DogOnt 1.0.5 Home Automation Ontology."@en ;
  owl:imports <http://www.w3.org/2006/time> ;
  owl:versionIRI <https://www.auto.tuwien.ac.at/downloads/thinkhome/ontology/energy/EnergyResourceOntology_1.03.owl> .

<http://www.semanticweb.org/owlapi#nodeID> a owl:AnnotationProperty .
rdfs:comment a owl:AnnotationProperty .
rdfs:label a owl:AnnotationProperty .
owl:versionInfo a owl:AnnotationProperty .
xsd:gDay a rdfs:Datatype .
xsd:gMonth a rdfs:Datatype .
<https://www.auto.tuwien.ac.at/downloads/thinkhome/ontology/EnergyResourceOntology.owl#areaUnitEnum>
  a rdfs:Datatype ;
  owl:equivalentClass [
    a rdfs:Datatype ;
    owl:oneOf (
     "SquareCentimeters"^^xsd:string
     "SquareFeet"^^xsd:string
     "SquareInches"^^xsd:string
     "SquareKilometers"^^xsd:string
     "SquareMeters"^^xsd:string
     "SquareMiles"^^xsd:string
     "SquareMillimeters"^^xsd:string
     "SquareYards"^^xsd:string
   )
  ] .

<https://www.auto.tuwien.ac.at/downloads/thinkhome/ontology/EnergyResourceOntology.owl#capacityUnitEnum>
  a rdfs:Datatype ;
  owl:equivalentClass [
    a rdfs:Datatype ;
    owl:oneOf (
     "Barrels"^^xsd:string
     "CubicMetres"^^xsd:string
     "Litres"^^xsd:string
     "LooseCubicMetres"^^xsd:string
     "Lumen"^^xsd:string
     "Steres"^^xsd:string
   )
  ] .

<https://www.auto.tuwien.ac.at/downloads/thinkhome/ontology/EnergyResourceOntology.owl#currencyTypeEnum>
  a rdfs:Datatype ;
  owl:equivalentClass [
    a rdfs:Datatype ;
    owl:oneOf (
     "AustralianDollar"^^xsd:string
     "CanadianDollars"^^xsd:string
     "EnglishPound"^^xsd:string
     "Euros"^^xsd:string
     "Franc"^^xsd:string
     "IndianRupee"^^xsd:string
     "Mark"^^xsd:string
     "NewZealandDollar"^^xsd:string
     "Pesos"^^xsd:string
     "Real"^^xsd:string
     "Ruble"^^xsd:string
     "TaiwanDollar"^^xsd:string
     "USDollars"^^xsd:string
     "Yen"^^xsd:string
     "Yuan"^^xsd:string
   )
  ] .

<https://www.auto.tuwien.ac.at/downloads/thinkhome/ontology/EnergyResourceOntology.owl#energyUnitEnum>
  a rdfs:Datatype ;
  owl:equivalentClass [
    a rdfs:Datatype ;
    owl:oneOf (
     "KiloWattHours"^^xsd:string
     "MegaWattHours"^^xsd:string
     "WattHours"^^xsd:string
     "WattPeaks"^^xsd:string
     "WattSeconds"^^xsd:string
   )
  ] .

<https://www.auto.tuwien.ac.at/downloads/thinkhome/ontology/EnergyResourceOntology.owl#weightUnitEnum>
  a rdfs:Datatype ;
  owl:equivalentClass [
    a rdfs:Datatype ;
    owl:oneOf (
     "Grams"^^xsd:string
     "KiloGrams"^^xsd:string
   )
  ] .

owl:topObjectProperty
  a owl:ObjectProperty ;
  rdfs:comment "maximum energy amount that a facility can produce"@en .

time:hasBeginning a owl:ObjectProperty .
time:hasEnd a owl:ObjectProperty .
time:inDateTime a owl:ObjectProperty .
time:unitType a owl:ObjectProperty .
<https://www.auto.tuwien.ac.at/downloads/thinkhome/ontology/EnergyResourceOntology.owl#actuallyConsumesEnergy>
  a owl:ObjectProperty ;
  rdfs:label "actuallyConsumesEnergy"^^xsd:string ;
  rdfs:comment "how much energy is consumed by an energy facility at a certain point in time"@en, "this property was known as \"currentlyConsumesEnergy\" in prior versions of the ontology"@en ;
  rdfs:subPropertyOf <https://www.auto.tuwien.ac.at/downloads/thinkhome/ontology/EnergyResourceOntology.owl#consumesEnergy> ;
  owl:propertyChainAxiom (
   <https://www.auto.tuwien.ac.at/downloads/thinkhome/ontology/EnergyResourceOntology.owl#hasCurrentStateValue>
   <https://www.auto.tuwien.ac.at/downloads/thinkhome/ontology/EnergyResourceOntology.owl#hasEnergyDemand>
 ) .

<https://www.auto.tuwien.ac.at/downloads/thinkhome/ontology/EnergyResourceOntology.owl#actuallyProducesEnergy>
  a owl:ObjectProperty ;
  rdfs:label "actuallyProducesEnergy"^^xsd:string ;
  rdfs:comment "The amount of energy a facility produces at a certain point in time"@en ;
  rdfs:subPropertyOf <https://www.auto.tuwien.ac.at/downloads/thinkhome/ontology/EnergyResourceOntology.owl#producesEnergy> .

<https://www.auto.tuwien.ac.at/downloads/thinkhome/ontology/EnergyResourceOntology.owl#actuatorOf>
  a owl:ObjectProperty ;
  owl:inverseOf <https://www.auto.tuwien.ac.at/downloads/thinkhome/ontology/EnergyResourceOntology.owl#hasActuator> .

<https://www.auto.tuwien.ac.at/downloads/thinkhome/ontology/EnergyResourceOntology.owl#ceilingOf> a owl:ObjectProperty .
<https://www.auto.tuwien.ac.at/downloads/thinkhome/ontology/EnergyResourceOntology.owl#commandOf>
  a owl:ObjectProperty ;
  owl:inverseOf <https://www.auto.tuwien.ac.at/downloads/thinkhome/ontology/EnergyResourceOntology.owl#hasCommand> .

<https://www.auto.tuwien.ac.at/downloads/thinkhome/ontology/EnergyResourceOntology.owl#consumesEnergy>
  a owl:ObjectProperty ;
  rdfs:label "consumesEnergy"^^xsd:string ;
  rdfs:comment "How much energy is consumed by a certain energy facility"@en ;
  rdfs:subPropertyOf owl:topObjectProperty ;
  rdfs:domain <https://www.auto.tuwien.ac.at/downloads/thinkhome/ontology/EnergyResourceOntology.owl#EnergyConsumerFacility> ;
  rdfs:range <https://www.auto.tuwien.ac.at/downloads/thinkhome/ontology/EnergyResourceOntology.owl#EnergyDemand> ;
  owl:propertyChainAxiom (
   <https://www.auto.tuwien.ac.at/downloads/thinkhome/ontology/EnergyResourceOntology.owl#hasState>
   <https://www.auto.tuwien.ac.at/downloads/thinkhome/ontology/EnergyResourceOntology.owl#hasStateValue>
   <https://www.auto.tuwien.ac.at/downloads/thinkhome/ontology/EnergyResourceOntology.owl#hasEnergyDemand>
 ) .

<https://www.auto.tuwien.ac.at/downloads/thinkhome/ontology/EnergyResourceOntology.owl#consumesEnergyType>
  a owl:ObjectProperty ;
  rdfs:label "consumesEnergyType"^^xsd:string ;
  rdfs:comment "The energy type that is consumed by a specific energy consuming facility"@en ;
  rdfs:subPropertyOf owl:topObjectProperty ;
  rdfs:domain <https://www.auto.tuwien.ac.at/downloads/thinkhome/ontology/EnergyResourceOntology.owl#EnergyConsumerFacility> ;
  rdfs:range <https://www.auto.tuwien.ac.at/downloads/thinkhome/ontology/EnergyResourceOntology.owl#FinalEnergy> ;
  owl:inverseOf <https://www.auto.tuwien.ac.at/downloads/thinkhome/ontology/EnergyResourceOntology.owl#energyTypeConsumedBy> ;
  owl:propertyChainAxiom (
   <https://www.auto.tuwien.ac.at/downloads/thinkhome/ontology/EnergyResourceOntology.owl#consumesEnergy>
   <https://www.auto.tuwien.ac.at/downloads/thinkhome/ontology/EnergyResourceOntology.owl#ofEnergyType>
 ) .

<https://www.auto.tuwien.ac.at/downloads/thinkhome/ontology/EnergyResourceOntology.owl#contains>
  a owl:ObjectProperty ;
  rdfs:label "contains"^^xsd:string ;
  rdfs:comment "Indicates which object are contained in a building environment, e.g. the kitchen contains Lamp1,Lamp2..."@en ;
  rdfs:domain <https://www.auto.tuwien.ac.at/downloads/thinkhome/ontology/EnergyResourceOntology.owl#BuildingEnvironment> ;
  rdfs:range [
    a owl:Class ;
    owl:unionOf (
     <https://www.auto.tuwien.ac.at/downloads/thinkhome/ontology/EnergyResourceOntology.owl#Appliance>
     <https://www.auto.tuwien.ac.at/downloads/thinkhome/ontology/EnergyResourceOntology.owl#BuildingAutomationSystemResource>
     <https://www.auto.tuwien.ac.at/downloads/thinkhome/ontology/EnergyResourceOntology.owl#BuildingEnvironment>
     <https://www.auto.tuwien.ac.at/downloads/thinkhome/ontology/EnergyResourceOntology.owl#Equipment>
   )
  ] .

<https://www.auto.tuwien.ac.at/downloads/thinkhome/ontology/EnergyResourceOntology.owl#controlledObject>
  a owl:ObjectProperty ;
  rdfs:label "controlledObject"^^xsd:string ;
  rdfs:comment "Association between a controller and the controlled object"@en .

<https://www.auto.tuwien.ac.at/downloads/thinkhome/ontology/EnergyResourceOntology.owl#energyActuallyConsumedBy>
